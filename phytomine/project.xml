<project type="bio">
  <property name="intermine.properties.file" value="phytomine.properties" />
  <property name="common.os.prefix" value="common" />
  <property name="target.model" value="genomic" />
  <property name="default.intermine.properties.file" location="../default.intermine.integrate.properties" />
  <property name="source.location" location="../bio/sources/" />
  <sources>
    <source name="cufflinks-P.vulgaris" type="cufflinks">
      <property name="proteomeId" value="218" />
      <property name="src.data.dir" location="/projectb/sandbox/plant/phytomine/rna-seq/Pvulgaris/" />
      <property name="dataSourceName" value="rnaseq-P.vulgaris" />
      <property name="dataSetTitle" value="P.vulgaris RNA-seq" />
    </source>
    <source name="cufflinks-C.reinhardtii" type="cufflinks">
      <property name="proteomeId" value="281" />
      <property name="src.data.dir" location="/projectb/sandbox/plant/phytomine/rna-seq/Creinhardtii/" />
      <property name="dataSourceName" value="rnaseq-C.reinhardtii" />
      <property name="dataSetTitle" value="C.reinhardtii RNA-seq" />
    </source>
    <source name="cufflinks-G.max" type="cufflinks">
      <property name="proteomeId" value="275" />
      <property name="src.data.dir" location="/projectb/sandbox/plant/phytomine/rna-seq/Gmax/" />
      <property name="dataSourceName" value="rnaseq-G.max" />
      <property name="dataSetTitle" value="G.max RNA-seq" />
    </source>
    <source name="cufflinks-E.grandis" type="cufflinks">
      <property name="proteomeId" value="201" />
      <property name="src.data.dir" location="/projectb/sandbox/plant/phytomine/rna-seq/Egrandis/" />
      <property name="dataSourceName" value="rnaseq-E.grandis" />
      <property name="dataSetTitle" value="E.grandis RNA-seq" />
    </source>

    <source name="cufflinks-P.trichocarpa-geneAtlas" type="cufflinks">
      <property name="proteomeId" value="210" />
      <property name="src.data.dir" location="/projectb/sandbox/plant/phytomine/rna-seq/Ptrichocarpa-geneAtlas/" />
      <property name="dataSourceName" value="rnaseq-P.trichocarpa-geneAtlas" />
      <property name="dataSetTitle" value="P.trichocarpa-geneAtlas RNA-seq" />
    </source>

    <source name="cufflinks-C.reinhardtii-geneAtlas" type="cufflinks">
      <property name="proteomeId" value="281" />
      <property name="src.data.dir" location="/projectb/sandbox/plant/phytomine/rna-seq/Creinhardtii-geneAtlas/" />
      <property name="dataSourceName" value="rnaseq-C.reinhardtii-geneAtlas" />
      <property name="dataSetTitle" value="C.reinhardtii-geneAtlas RNA-seq" />
    </source>

    <source name="cufflinks-P.patens-geneAtlas" type="cufflinks">
      <property name="proteomeId" value="251" />
      <property name="src.data.dir" location="/projectb/sandbox/plant/phytomine/rna-seq/Ppatens-geneAtlas/" />
      <property name="dataSourceName" value="rnaseq-P.patens-geneAtlas" />
      <property name="dataSetTitle" value="P.patens-geneAtlas RNA-seq" />
    </source>

  </sources>
  <post-processing>
    <post-process name="create-references" />
    <post-process name="transfer-sequences" />
    <!-- this runs the postprocessing steps in all sources -->
    <post-process name="do-sources" />
    <post-process name="create-gene-flanking-features" />
    <post-process name="summarise-objectstore" />
    <post-process name="create-autocomplete-index" />
    <post-process name="create-search-index" />
    <post-process name="add-uniprot-ids" />
    <post-process name="transfer-go-terms" />
    <post-process name="transfer-ontology-terms" />
    <post-process name="name-features" />

    
  </post-processing>
</project>
